function get_curr_cycle_time(start_time)
    
    curr_time = GetReg(285); -- T0+ (T0@Internal WebHMI)
    curr_cycle_time = curr_time - start_time;
    return curr_cycle_time
end
 
function is_cycle_running(curr_cycle_time, max_cycle_time)
    
    if( curr_cycle_time >= max_cycle_time ) then
        return 0            
    else
        return 1
    end
end



